{
  "name": "wdwc-ethereum",
  "version": "1.0.0",
  "description": "Wrapped DarkWave Coin (wDWC) Upgradeable ERC-20 for Ethereum - DSC Bridge",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
