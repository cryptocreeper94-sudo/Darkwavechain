  "services": {
    "hashSubmission": {
      "description": "Submit data hashes to DarkWave Chain for immutable timestamping",
      "endpoint": "POST /api/hash/submit",
      "authentication": "X-API-Key header",
      "requestBody": {
        "dataHash": "string (required) - SHA-256 or Keccak-256 hash",
        "metadata": "object (optional) - Custom metadata"
      },
      "response": {
        "txHash": "Transaction hash on DarkWave Chain",
        "blockHeight": "Block number containing the transaction",
        "timestamp": "ISO 8601 timestamp",
        "confirmations": "Number of confirmations",
        "gasUsed": "Gas units consumed"
      }
    },
    "dualChainStamping": {
      "description": "Stamp data to both DarkWave Chain and Solana simultaneously",
      "endpoint": "POST /api/stamp/dual",
      "authentication": "X-API-Key header",
      "requestBody": {
        "dataHash": "string (required) - Data hash to stamp",
        "metadata": "object (optional) - Custom metadata"
      },
      "response": {
        "stampId": "Unique stamp identifier",
        "darkwave": {
          "txHash": "DarkWave transaction hash",
          "status": "confirmed | pending",
          "blockHeight": "Block number"
        },
        "solana": {
          "status": "pending_client_submission | confirmed",
          "signature": "Solana transaction signature (after update)"
        }
      },
      "workflow": [
        "1. Submit to /api/stamp/dual with your hash",
        "2. DarkWave Chain submission happens server-side automatically",
        "3. Use returned memoData to submit to Solana client-side",
        "4. Update stamp with Solana signature via PATCH /api/stamp/{stampId}/solana",
        "5. Both chains now contain your timestamped data"
      ]
    },
    "hallmark": {
      "description": "Generate unique, verifiable product identifiers",
      "format": "XXXXXXXXX-YY (9-digit master + 2-digit sub-sequence)",
      "endpoints": {
        "generate": "POST /api/hallmark/generate",
        "verify": "GET /api/hallmark/{hallmarkId}/verify",
        "qrCode": "GET /api/hallmark/{hallmarkId}/qr",
        "details": "GET /api/hallmark/{hallmarkId}"
      },
      "features": [
        "Auto-generated QR codes (SVG)",
        "On-chain verification via DarkWave Chain",
        "Verification status tracking (pending â†’ confirmed)",
        "Full audit trail with timestamps"
      ]
    },
    "gasEstimation": {
      "description": "Estimate gas costs for transactions",
      "endpoint": "GET /api/gas/estimate",
      "queryParams": {
        "dataSize": "Size of data in bytes"
      },
      "feeSchedule": {
        "baseFee": 21000,
        "hashSubmission": 25000,
        "perByte": 16,
        "hallmarkGeneration": 50000,
        "dualChainStamp": 75000
      }
    }
  },
  "authentication": {
    "developerPin": {
      "description": "Initial authentication using developer PIN",
      "endpoint": "POST /api/developer/auth",
      "requestBody": {
        "pin": "4-digit developer PIN"
      },
      "response": {
        "sessionToken": "Temporary session token (1 hour)",
        "expiresIn": 3600
      }
    },
    "apiKeyRegistration": {
      "description": "Register for a permanent API key",
      "endpoint": "POST /api/developer/register",
      "headers": {
        "X-Developer-Session": "Session token from PIN auth"
      },
      "requestBody": {
        "appName": "Your application name",
        "email": "Contact email for notifications"
      },
      "response": {
        "apiKey": "Permanent API key (dwc_live_...)",
        "appId": "Your application ID"
      }
    }
  },
